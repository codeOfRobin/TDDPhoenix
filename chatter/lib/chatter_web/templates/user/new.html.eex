
<%= form_for @changeset, Routes.user_path(@conn, :create), fn f -> %>
  <label>
    Email: <%= text_input f, :email %>
    <%= error_tag f, :email %>
  </label>

  <label>
    Password: <%= password_input f, :password %>
     <%= error_tag f, :password %>  </label>
  </label>

  <%= submit "Sign up" %>
<% end %>
